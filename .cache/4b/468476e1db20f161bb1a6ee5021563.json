-box;\r\n}\r\nbody{\r\n  background-color: #282a35;\r\n  font-family: 'Roboto', sans-serif;\r\n}\r\n\r\n/* Top Bar */\r\n.topBar{\r\n  display: flex;\r\n  background-color: #e50914;\r\n}\r\n.topBar-left{\r\n  flex: 40%;\r\n  padding: 1rem 3rem;\r\n}\r\n.topBar-left #websiteTitle{\r\n  color: #ffffff;\r\n  font-weight: bold;\r\n  border: 0;\r\n  outline: none;\r\n  font-size: 2rem;\r\n  background-color: transparent;\r\n  text-decoration: none;\r\n}\r\n#websiteTitle i{\r\n  color: #ffffff;\r\n  padding: 0.5rem;\r\n  font-weight: bold;\r\n  font-size: 2rem;\r\n}\r\n.topBar-right{\r\n  flex: 60%;\r\n  display: flex;\r\n  justify-content: right;\r\n  align-items: center;\r\n  padding-right: 3rem;\r\n  color: #ffffff;\r\n  font-size: 1.2rem;\r\n  font-weight: bold;\r\n}\r\n\r\n/* Currency  Calculator */\r\n.currencyCalculator{\r\n  padding: 3rem;\r\n  background-color: #20325a;\r\n  margin: 3rem auto;\r\n  box-shadow: 15px 15px #f1f1f1;\r\n  width: 80%;\r\n  \r\n}\r\n.formTop{\r\n  display:flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.formBottom{\r\n  display:flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding-top: 2rem;\r\n}\r\n.currencyExchangeTitle{\r\n  padding: 2rem;\r\n  font-size: 3rem;\r\n  color: #ffffff;\r\n}\r\n#currencyAmount{\r\n  width: 50%;\r\n}\r\n#currencyAmount:focus, #currencySource:focus, #currencyTarget:focus{\r\n  border: 3px solid #e50914;\r\n}\r\n#inputErrMsg{\r\n  padding-top: 1rem;\r\n  font-size: 1rem;\r\n  font-weight: bold;\r\n  color: #e50914;\r\n}\r\n#changeCurrencyButton, #convertCurrencyButton{\r\n  padding: 0.3rem 2rem;\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n  margin: 1rem;\r\n  color: #ffffff;\r\n  background-color: #e50914;\r\n}\r\n#currencySource, #currencyTarget{\r\n  width: 35%;\r\n  margin: 0.625rem;\r\n}\r\n#currencyAmount, #currencySource, #currencyTarget{\r\n  padding: 0.5rem;\r\n  font-size: 2rem;\r\n}\r\n#resultSection{\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  color: #ffffff;\r\n}\r\n#exchangeResult{\r\n  font-size: 3rem;\r\n  font-weight: bold;\r\n  padding-top: 4rem;\r\n  padding-bottom: 2rem;\r\n  font-family: Arial, Helvetica, sans-serif;\r\n}\r\n#conversionRate{\r\n  font-size: 2rem;\r\n  font-weight: bold;\r\n}\r\n\r\n\r\n/* Currency Exchange Rate Table */\r\n.currenciesRates{\r\n  display: flex;\r\n  justify-content: center;\r\n  padding: 3rem;\r\n  margin: 3rem;\r\n}\r\n#exchangeTable{\r\n  width: 70%;\r\n  height: 25rem;\r\n  border-collapse: collapse;\r\n  text-align: center;\r\n}\r\n#exchangeTable caption{\r\n  font-size: 3rem;\r\n  padding: 2rem;\r\n  font-weight: bold;\r\n  color: #ffffff;\r\n}\r\n#exchangeTable th{\r\n  background-color: #e50914;\r\n  color: #ffffff;\r\n  font-size: 1rem;\r\n}\r\n#exchangeTable th, #exchangeTable td{\r\n  padding: 0.5rem;\r\n}\r\n#exchangeTable tr:not(th){\r\n  background-color: #f1f1f1;\r\n}\r\n#exchangeTable tr:nth-child(odd){\r\n  background-color: #cccccc;\r\n}\r\n#exchangeTable td{\r\n  font-size: 1rem;\r\n  font-weight: bold;\r\n  color: #20325a;\r\n}\r\n\r\n/* Economic News Section */\r\n.financialNewsSection{\r\n  display: flex;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  padding-bottom: 3rem;\r\n  margin-bottom: 3rem;\r\n}\r\n#financialNewsTitle{\r\n  padding: 2rem;\r\n  color: #ffffff;\r\n  font-size: 3rem;\r\n  font-weight: bold;\r\n  text-align: center;\r\n}\r\n.financialNewsTabs{\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.financialNewsTabs .newsTab{\r\n  width: 20rem;\r\n  height: 30rem;\r\n  background-color: #ffffff;\r\n  margin: 1rem;\r\n  padding: 1rem;\r\n  border-radius: 10px;\r\n  overflow-y: scroll;\r\n}\r\n.newsTabTitle{\r\n font-size: 1rem;\r\n margin: 1rem 0;\r\n border-bottom: 3px solid #e50914;\r\n}\r\n.newsTabParagraph{\r\n font-size: 1rem;\r\n margin-top: 2rem;\r\n font-family: Arial, Helvetica, sans-serif;\r\n}\r\n\r\n/* Footer */\r\nfooter{\r\n  background-color: #e50914;\r\n  padding: 1.5rem;\r\n  color: #ffffff;\r\n  display: flex;\r\n}\r\n"},"lineCount":207}},"error":null,"hash":"f96b0826a1d6c75596ef77f7845cf88e","cacheData":{"env":{}}}